program cariAwaldanAkhir;
var
        kata, hurufAwal, HurufAkhir : string;
procedure cari(kt:string);
Begin
        readln(kt);
        writeln('Huruf ' , hurufAwal , ' adalah huruf Pertama.');
        writeln('Huruf ' , hurufAkhir , ' adalah huruf Terakhir.');

end;

Begin
        write('Masukkan kata : ');
        cari(kata);
        readln;
End.